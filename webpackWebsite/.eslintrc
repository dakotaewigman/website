{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        },
        "allowImportExportEverywhere": true
    },
    "ignorePatterns": ["node_modules/", "dist/"],
    "extends": [
        "airbnb"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "rules": {

    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}